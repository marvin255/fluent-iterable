{
    "$schema": "vendor/infection/infection/resources/schema.json",
    "source": {
        "directories": [
            "src"
        ]
    },
    "logs": {
        "text": "infection.log"
    },
    "mutators": {
        "@default": true,
        "Break_": {
            "ignore": [
                "Marvin255\\FluentIterable\\Helper\\DataAccessor::get"
            ]
        },
        "LessThan": {
            "ignore": [
                "Marvin255\\FluentIterable\\Helper\\Reducer::min"
            ]
        },
        "GreaterThan": {
            "ignore": [
                "Marvin255\\FluentIterable\\Helper\\Reducer::max"
            ]
        }
    }
}